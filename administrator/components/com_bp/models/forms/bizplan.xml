<?xml version="1.0" encoding="utf-8"?>
<form>
  <fieldset>
    <field name="id" type="text" readonly="true" class="readonly" default="0" description="JGLOBAL_FIELD_ID_DESC"/>
    <field name="ordering"/>
    <field name="state" default="1" type="list" label="JSTATUS" description="JFIELD_PUBLISHED_DESC" class="inputbox" size="1">
      <option value="1">JPUBLISHED</option>
      <option value="0">JUNPUBLISHED</option>
      <option value="2">JARCHIVED</option>
      <option value="-2">JTRASHED</option>
    </field>
    <field name="checked_out" type="hidden" filter="unset"/>
    <field name="checked_out_time" default="0000-00-00 00:00:00" type="hidden" filter="unset"/>
    <field name="created_by" type="createdby" default="0" label="COM_BP_FORM_LBL_BIZPLAN_CREATED_BY" description="COM_BP_FORM_DESC_BIZPLAN_CREATED_BY" hidden="true" hint="COM_BP_FORM_LBL_BIZPLAN_CREATED_BY"/>
    <field name="modified_by" type="modifiedby" default="0" label="COM_BP_FORM_LBL_BIZPLAN_MODIFIED_BY" description="COM_BP_FORM_DESC_BIZPLAN_MODIFIED_BY" hidden="true" hint="COM_BP_FORM_LBL_BIZPLAN_MODIFIED_BY"/>
    <field name="name" type="text" required="true" label="COM_BP_FORM_LBL_BIZPLAN_NAME" description="COM_BP_FORM_DESC_BIZPLAN_NAME" hint="COM_BP_FORM_LBL_BIZPLAN_NAME"/>
    <field name="category" extension="com_content" type="category" label="COM_BP_FORM_LBL_BIZPLAN_CATEGORY" description="COM_BP_FORM_DESC_BIZPLAN_CATEGORY" hint="COM_BP_FORM_LBL_BIZPLAN_CATEGORY"/>
    <field name="description" type="textarea" label="COM_BP_FORM_LBL_BIZPLAN_DESCRIPTION" description="COM_BP_FORM_DESC_BIZPLAN_DESCRIPTION" hint="COM_BP_FORM_LBL_BIZPLAN_DESCRIPTION"/>
    <field name="lang" type="language" label="COM_BP_FORM_LBL_BIZPLAN_LANG" description="COM_BP_FORM_DESC_BIZPLAN_LANG" hint="COM_BP_FORM_LBL_BIZPLAN_LANG"/>
    <field name="address" type="text" label="COM_BP_FORM_LBL_BIZPLAN_ADDRESS" description="COM_BP_FORM_DESC_BIZPLAN_ADDRESS" hint="COM_BP_FORM_LBL_BIZPLAN_ADDRESS"/>
    <field name="lat" default="41.310860" label="COM_BP_FORM_LBL_BIZPLAN_LAT" description="COM_BP_FORM_DESC_BIZPLAN_LAT" hint="COM_BP_FORM_LBL_BIZPLAN_LAT"/>
    <field name="lng" default="69.279335" label="COM_BP_FORM_LBL_BIZPLAN_LNG" description="COM_BP_FORM_DESC_BIZPLAN_LNG" hint="COM_BP_FORM_LBL_BIZPLAN_LNG"/>
    <field name="votes_count" default="0" label="COM_BP_FORM_LBL_BIZPLAN_VOTES_COUNT" description="COM_BP_FORM_DESC_BIZPLAN_VOTES_COUNT" hint="COM_BP_FORM_LBL_BIZPLAN_VOTES_COUNT"/>
    <field name="loyiha_qiymati" label="COM_BP_FORM_LBL_BIZPLAN_LOYIHA_QIYMATI" description="COM_BP_FORM_DESC_BIZPLAN_LOYIHA_QIYMATI" hint="COM_BP_FORM_LBL_BIZPLAN_LOYIHA_QIYMATI"/>
    <field name="tuplangan_mablag" label="COM_BP_FORM_LBL_BIZPLAN_TUPLANGAN_MABLAG" description="COM_BP_FORM_DESC_BIZPLAN_TUPLANGAN_MABLAG" hint="COM_BP_FORM_LBL_BIZPLAN_TUPLANGAN_MABLAG"/>
    <field name="mediamanager" directory="bp_images" type="media" label="COM_BP_FORM_LBL_BIZPLAN_MEDIAMANAGER" description="COM_BP_FORM_DESC_BIZPLAN_MEDIAMANAGER" hint="COM_BP_FORM_LBL_BIZPLAN_MEDIAMANAGER"/>
    <field name="feautured_0_255" label="COM_BP_FORM_LBL_BIZPLAN_FEAUTURED_0_255" description="COM_BP_FORM_DESC_BIZPLAN_FEAUTURED_0_255" hint="COM_BP_FORM_LBL_BIZPLAN_FEAUTURED_0_255"/>
    <field name="version_note" type="text" label="JGLOBAL_FIELD_VERSION_NOTE_LABEL" description="JGLOBAL_FIELD_VERSION_NOTE_DESC" class="inputbox" size="45" labelclass="control-label"/>
  </fieldset>
  <fieldset name="accesscontrol">
    <field name="asset_id" type="hidden" filter="unset"/>
    <field name="rules" type="rules" label="JFIELD_RULES_LABEL" translate_label="false" filter="rules" validate="rules" class="inputbox" component="com_bp" section="bizplan"/>
  </fieldset>
</form>
